# {{ ansible_managed }}
{% if set_domain  %}
{% if domain %}
domain {{domain}}
{% endif %}
{% endif %}
{% if set_nameservers %}
{% if ResolverCheck.rc == 0 %}
{% for dnsserver in dns_nameservers %}
nameserver {{dnsserver}}
{% endfor %}
{% endif %}
{% if ResolverCheck.rc != 0 %}
{{ default_nameservers_description }}
{% for dnsserver in default_nameservers %}
nameserver {{dnsserver}}
{% endfor %}
{% endif %}
{% endif %}
{% if set_searchs %}
search {% for suffix in dns_searchs %}{{suffix}} {% endfor %}
{% endif %}
