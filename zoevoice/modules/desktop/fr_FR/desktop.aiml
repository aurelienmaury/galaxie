<!-- Firefox -->
<category>
    <pattern>LANCE UN NAVIGATEUR</pattern>
    <template><system>../modules/desktop/desktop.py --browser</system></template>
</category>
<category><pattern>NAVIGATEUR</pattern><template><srai>LANCE UN NAVIGATEUR</srai></template></category>
<category><pattern>* NAVIGATEUR</pattern><template><srai>LANCE UN NAVIGATEUR</srai></template></category>
<category><pattern>NAVIGATEUR INTERNET</pattern><template><srai>LANCE UN NAVIGATEUR</srai></template></category>
<category><pattern>* NAVIGATEUR INTERNET</pattern><template><srai>LANCE UN NAVIGATEUR</srai></template></category>
<category><pattern>NAVIGATEUR W*</pattern><template><srai>LANCE UN NAVIGATEUR</srai></template></category>
<category><pattern>* NAVIGATEUR W*</pattern><template><srai>LANCE UN NAVIGATEUR</srai></template></category>

<!-- Thunderbird -->
<category>
    <pattern>COURRIER</pattern>
    <template><system>../modules/desktop/desktop.py --email</system></template>
</category>
<category><pattern>* COURRIER</pattern><template><srai>COURRIER</srai></template></category>

<!-- Terminal -->
<category>
    <pattern>TERMINAL</pattern>
    <template><system>../modules/desktop/desktop.py --terminal</system></template>
</category>
<category><pattern>* TERMINAL</pattern><template><srai>TERMINAL</srai></template></category>
<category><pattern>* TERMINALE</pattern><template><srai>TERMINAL</srai></template></category>
<category><pattern>TERMINALE</pattern><template><srai>TERMINAL</srai></template></category>
</aiml>
